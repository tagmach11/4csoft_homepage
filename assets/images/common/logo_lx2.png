 <script type="text/javascript">

 	function loginPop(returnUrl) {

		var action = $.action();
		action.config.formId = "FormGlobalParameters";
		action.config.url    = "https://lx2.kr/common/login/loginpage.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
		var form = UT.getById(action.config.formId);
		if (returnUrl == null || returnUrl == '') {
			returnUrl = form.elements["returnUrl"].value;
		}

   		form.elements["loginReturnUrl"].value = returnUrl;
		action.run();
	};


	 var COURSE_ITEM = {
		doGoCourseDetail : function(params){
			var action = $.action("normal", {formId : "FormGlobalParameters"});
			action.config.url    = "/course/active/detail.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2?courseMasterSeq="+params.courseMasterSeq+"&courseActiveSeq="+params.courseActiveSeq;
		    UT.copyValueMapToForm(params, action.config.formId);
			action.run();
		},
		doGoSmallLearningDetail : function(params){
			//if(sessionAlive) {
				var action = $.action("normal", {formId : "FormGlobalParameters"});
				action.config.url    = "/small/learning/detail.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2?courseMasterSeq="+params.courseMasterSeq+"&courseActiveSeq="+params.courseActiveSeq;
				UT.copyValueMapToForm(params, action.config.formId);
				action.run();
			//} else{
			//	loginPop('');
			//}
		},
		/*
		강의계획서
		*/
		doGoCourseDetailPopup : function(params){
			var action = $.action("layer", {formId : "FormGlobalParameters"});
			action.config.url    = "/course/active/detail/popup.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2?courseMasterSeq="+params.courseMasterSeq+"&courseActiveSeq="+params.courseActiveSeq;
			action.config.options.width  = 1000;
			action.config.options.height = 700;
			action.config.options.title = "Syllabus";
		    UT.copyValueMapToForm(params, action.config.formId);
			action.run();
		},
		
		doApplyRequest : function(mapPKs) {
			var formClassroom = UT.getById("FormClassroom");
			formClassroom.elements["courseActiveSeq"].value = mapPKs.courseActiveSeq;
			formClassroom.elements["courseApplyTmplSeq"].value = mapPKs.courseApplyTmplSeq;

			if(sessionAlive) {
				if (mapPKs.passwordYn == "Y" && mapPKs.passwordTypeCd == "COURSE_PASSWORD_TYPE::001" && mapPKs.passYn !="Y") {
					$("#pop_chk_apply_pass").fadeIn();
				} else {
					
					if (mapPKs.surveyPassYn != "Y") {
						COURSE_ITEM.doSurvey(mapPKs, 'before', 'COURSE_SURVEY_JOIN_TYPE::001');
					}
					
					if (mapPKs.surveyPassYn == "Y") {
						if (mapPKs.courseApplyTmplSeq <= 0) {
							action = $.action("ajax", {formId : "FormApplyInsert"});
							action.config.url             = "/usr/course/apply/non/insert/json.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
							action.config.type        	= "json";
							action.config.message.confirm = "Would you like to Enroll for the course?";
							action.config.message.waiting = "Course registration in progress.";
							action.config.fn.complete     = function(action, data) {
								if (data != null && data.result != null) {
									if(data.result=='-11'){
										doReadySystem("정원이 초과되었습니다.");
									} else if (data.result=='-95') {
										doReadySystem("Not a learning target course.");
									}else{
										window.location.href='/usr/member/dash/detail.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2';
									}
						        } else {
						            FN.doErrorMessage();
						        }
							};
							UT.getById(action.config.formId).reset();
							UT.copyValueMapToForm(mapPKs, action.config.formId);
							action.run();
						} else {
							//수강신청서
							var action = $.action("ajax",{formId : "FormApplyInsert"});
						     action.config.type = "html";
						     action.config.url    = "/usr/courseApplyTmpl/create/popup.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
						     action.config.containerId = "ct_courseApplyTmplPopup";
						     UT.copyValueMapToForm(mapPKs, action.config.formId);
						     action.config.fn.complete = function(act, data) {
						    	 $("#courseApplyTmplPopup").fadeIn();
						     };
						     UT.copyValueMapToForm(mapPKs, action.config.formId);
						     action.run();
						}
					}

				}

			}else{
				loginPop('');
			}
		},
		 doSurvey : function(mapPKs, elementType, courseSurveyJoinType) {
			action = $.action("ajax", {formId : "FormSurveyDetail"});
			action.config.url             = "/usr/course/active/surveypaper/elementType/check/json.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2".replace("elementType",elementType);
			action.config.type        	= "json";
			action.config.fn.complete     = function(action, data) {
				if (data != null && data.result != null) {
					if(data.result=='-1') {
						if (courseSurveyJoinType == 'COURSE_SURVEY_JOIN_TYPE::001') {
							mapPKs.surveyPassYn = "Y";
							COURSE_ITEM.doApplyRequest(mapPKs);	
						} else {
							mapPKs.surveyClassPassYn = "Y";
							COURSE_ITEM.doClassroom(mapPKs);	
						}
						//doReadySystem("출제된 설문지가 없습니다.");
					} else {
						//설문참여
						if (data.submitYn != 'Y') {
							var action2 = $.action("ajax",{formId : "FormSurveyDetail"});
						     action2.config.type = "html";
						     action2.config.url    = "/usr/course/active/survey/paper/elementType/ajax.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2".replace("elementType",elementType);
						     action2.config.containerId = "ct_surveyPopup";
						     action2.config.fn.complete = function(act, data2) {
						    	 $("#surveyPopup").fadeIn();
						    	 SUB.initPage();
						     };
						     
						     
						     var form = UT.getById(action2.config.formId);
						     form.elements["mandatoryYn"].value = data.mandatoryYn;
						     form.elements["courseActiveSeq"].value = data.courseActiveSeq;
						     form.elements["courseActiveSurveySeq"].value = data.courseActiveSurveySeq;
						     form.elements["courseApplyTmplSeq"].value = mapPKs.courseApplyTmplSeq;
						     form.elements["courseSurveyJoinType"].value = courseSurveyJoinType;
						     
						     action2.run();
						} else {
							//설문참여 완료
							if (courseSurveyJoinType == 'COURSE_SURVEY_JOIN_TYPE::001') {
								mapPKs.surveyPassYn = "Y";
								COURSE_ITEM.doApplyRequest(mapPKs);	
							} else {
								mapPKs.surveyClassPassYn = "Y";
								COURSE_ITEM.doClassroom(mapPKs);	
							}
						}
						
						
					}
		        } else {
		            FN.doErrorMessage();
		        }
			};
			UT.getById(action.config.formId).reset();
			UT.copyValueMapToForm(mapPKs, action.config.formId);
			
			var form = UT.getById(action.config.formId);
		    form.elements["courseSurveyJoinType"].value = courseSurveyJoinType;
			action.run();
			
		},
		 doExam : function(mapPKs, elementType) {
			
			 	action = $.action("ajax", {formId : "FormExamDetail"});
				action.config.url             = "/usr/course/active/exampaper/elementType/check/json.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2".replace("elementType",elementType);
				action.config.type        	= "json";
				action.config.fn.complete     = function(action, data) {
					
					if (data != null && data.result != null) {
						if(data.result=='-1') {
								mapPKs.examClassPassYn = "Y";
								COURSE_ITEM.doClassroom(mapPKs);	
							//doReadySystem("출제된 시험지가 없습니다.");
						} else {
							//시험 참여
							if (data.submitYn != 'Y') {

								var action2 = $.action("layer",{formId : "FormExamDetail"});
							     action2.config.type = "html";
							     action2.config.url    = "/usr/course/exampaper/detail/elementType/popup.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2".replace("elementType",elementType);
							     action2.config.options.width  = 1450;
							     action2.config.options.height  = 920;
							     action2.config.options.position = "middle";
							     
							     
							     var form = UT.getById(action2.config.formId);
							     form.elements["courseActiveSeq"].value = data.courseActiveSeq;
							     form.elements["courseApplySeq"].value = data.courseApplySeq;
							     form.elements["courseActiveExamPaperSeq"].value = data.courseActiveExamPaperSeq;
							     form.elements["activeElementSeq"].value = data.activeElementSeq;
							     form.elements["examPaperSeq"].value = data.examPaperSeq;
							     form.elements["callback"].value = "COURSE_ITEM.doClassroom({'surveyClassPassYn' : 'Y', 'examClassPassYn' : 'Y', 'passYn' : 'Y', 'courseApplyTmplSeq' : '-1', 'courseActiveSeq' : '"+data.courseActiveSeq+"', 'courseApplySeq' : '"+data.courseApplySeq+"'});";
							     form.elements["courseApplyTmplSeq"].value = mapPKs.courseApplyTmplSeq;
							     action2.run();
							} else {
								//시험 참여 완료
								mapPKs.examClassPassYn = "Y";
								COURSE_ITEM.doClassroom(mapPKs);	
							}
							
						}
			        } else {
			            FN.doErrorMessage();
			        }
				};
				UT.getById(action.config.formId).reset();
				UT.copyValueMapToForm(mapPKs, action.config.formId);

				action.run();
			
		},
		 doClassroom : function(mapPKs) {

			UT.getById("FormClassroom").reset();
			UT.copyValueMapToForm(mapPKs, "FormClassroom");

			if (mapPKs.passwordYn == "Y" && mapPKs.passwordTypeCd == "COURSE_PASSWORD_TYPE::002" && mapPKs.passYn !="Y") {
				UT.copyValueMapToForm(mapPKs, "FormCoursePassword");
				$("#pop_chk_course_pass").fadeIn();
			} else {
				
				if (mapPKs.surveyClassPassYn == 'Y') {
					if (mapPKs.examClassPassYn == 'Y') {
						
						var action2 = $.action();
						action2.config.formId = "FormClassroom";
						action2.config.url = "/usr/classroom/main.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
						action2.run();
					} else {
						mapPKs.courseApplyTmplSeq = -1;
						COURSE_ITEM.doExam(mapPKs, 'before');
					}
				} else {
					mapPKs.courseApplyTmplSeq = -1;
					COURSE_ITEM.doSurvey(mapPKs, 'before', 'COURSE_SURVEY_JOIN_TYPE::002');
				}
				
			}
		},
		doSmallLearningClassroom : function(mapPKs) {
			var action = $.action();
			action.config.formId = "FormSmallLearningClassroom";
			action.config.url = "/small/learning/sclassroom/main.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
			
			UT.getById(action.config.formId).reset();
			UT.copyValueMapToForm(mapPKs, action.config.formId);
			
			action.run();
		},
		doPreview : function(mapPKs) {
		 	
			var form = UT.getById("form-main-preview");
	        form.elements["courseActiveTitle"].value = mapPKs.courseActiveTitle;
	        form.elements["organizationSeq"].value = mapPKs.organizationSeq;
			
			action = $.action("ajax", {formId : "form-main-preview"});
			action.config.url             = "/orgainization/preview/org/check/json.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
			action.config.type        	= "json";
			action.config.fn.complete     = function(action, data) {
				if (data != null && data.result != null) {
					// OER 분기
					
			        form.elements["courseActiveSeq"].value = mapPKs.courseActiveSeq;
			        form.elements["itemSeq"].value = data.itemSeq;
			        form.elements["itemIdentifier"].value = data.itemIdentifier;
			        form.elements["contentsTypeCd"].value = data.contentsTypeCd;
			        form.elements["href"].value = data.href;
			        form.elements["href"].value = data.href;
				        
					if(data.contentsTypeCd == "CONTENTS_TYPE::OER"){
				    	var action2 = $.action("layer");
				    	action2.config.formId = "form-main-preview";
				    	action2.config.url            = "/usr/orgainization/preview.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
				    	action2.config.options.title  = "미리보기";
				    	action2.config.options.width = 1000;
				    	action2.config.options.height = 600;
				    	
				    	
				    	action2.run();

				 	} else{
				    	var action2 = $.action("ajax", {formId : "form-main-preview"});
				    	action2.config.formId = "form-main-preview";
				    	action2.config.type = "html";
				    	action2.config.containerId = "ct";
				    	action2.config.url            = "/orgainization/preview.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
				    	action2.config.fn.complete     = function(action2, data) {
				        	$('#pop_preview').show();
						};
						action2.run();
				 	}
		        } else {
		            FN.doErrorMessage();
		        }
			};
			
			action.run();
			
		 },
		 doClosePerview : function(){
			 setTimeout(function() {
				 $('#ct').empty();
				}, 500);
		 },
		 doGoOpenLecture : function(params){
			var action = $.action("normal", {formId : "FormGlobalParameters"});
			action.config.url    = "/course/active/detail.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2?courseMasterSeq="+params.courseMasterSeq+"&courseActiveSeq="+params.courseActiveSeq;
			action.config.url    = "/open/open/lecture/detail.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2?openLectureSeq="+params.openLectureSeq;
		    UT.copyValueMapToForm(params, action.config.formId);
			action.run();
		},
		doApplyPassword : function() {
			var form = UT.getById("FormApplyPassword");	
			var formClassroom = UT.getById("FormClassroom");	
			form.elements["courseActiveSeq"].value = formClassroom.elements["courseActiveSeq"].value;
			
			var action = $.action("ajax");
			action.config.type   = "json";
			action.config.formId = "FormApplyPassword";
			action.config.url    = "/usr/course/active/check/password/json.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2";
		  	action.config.fn.validate = function() {
				if (Global.validator.checker["!null"](form.elements['password'].value) == false) {
					$.dialog("alert", {message : "Please enter your password."});
					return false;	
				}
				return true;
			};
			action.config.fn.complete = function(action, data) {
				if (data.success != null && data.success > 0) {
					$("#pop_chk_apply_pass").fadeOut();
					COURSE_ITEM.doApplyRequest({'surveyPassYn' : 'N', 'passYn' : 'Y', 'courseApplyTmplSeq' : formClassroom.elements["courseApplyTmplSeq"].value, 'courseActiveSeq' : formClassroom.elements["courseActiveSeq"].value});
				} else {
					$.dialog("alert", {message : "Password does not match."});
					form.elements["password"].value = null;
				}
			};
			action.run();
		},
		doApplyPasswordClose : function(){
			$("#pop_chk_apply_pass").fadeOut();
		}

	};

	pageOpen = new Date();
	function bye() {
		pageClose = new Date();
		minutes = (pageClose.getMinutes() - pageOpen.getMinutes());
		seconds = (pageClose.getSeconds() - pageOpen.getSeconds());
		time = (seconds + (minutes * 60));
		var url = window.location.pathname;
		//alert('저희 홈페이지에 ' + time + '초 동안 머무르셨습니다');
		console.log(url + ' 페이지에 ' + time + '초 동안 머무르셨습니다');
	}

	window.onbeforeunload = function (event) {

        	
		pageclose(); // Analytics call
        
		
		if( typeof(API) != 'undefined' ) {
			API.Terminate("");
		}

		bye();
	}

	function pageopen() {

		var form = UT.getById("form-analytics");

        jQuery.ajax({
            url : "https://lx2.kr/api/analytics/pageopen.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2",
            cache : false,
            dataType : 'json',
            data : {
                url : window.location.href,
                referer : document.referrer
            },
            success : function(res){
				//console.log("res:" + res.uuid)
				form.elements["id"].value = res.id;
            }
        });

	}

	function pageclose() {

		var form = UT.getById("form-analytics");

        jQuery.ajax({
            url : "https://lx2.kr/api/analytics/pageclose.do;jsessionid=F7009E7CEE542552CF521287A1532A82.was2",
            cache : false,
            dataType : 'json',
            data : {
                id : form.elements["id"].value
            },
            success : function(res){
				console.log("res:" + res.result)
            }
        });
	}

	jQuery(document).ready(function(){
		
        
			pageopen();  // Analytics call
        
	});

